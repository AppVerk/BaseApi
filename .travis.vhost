<VirtualHost *:80>

  DocumentRoot %TRAVIS_BUILD_DIR%/web
  ServerName %VHOST_URL%

  <Directory "%TRAVIS_BUILD_DIR%/web">
    AllowOverride All
    Order Allow,Deny
    Allow from all
  </Directory>

  ErrorLog ${APACHE_LOG_DIR}/%VHOST_URL%.error.log
  CustomLog ${APACHE_LOG_DIR}/%VHOST_URL%.access.log common

</VirtualHost>